		<script>
			function showPopup(ele) {
				var container = ele.closest('.container');
				var popup = container.find('.popup');
				popup.show('fadeIn');
			}
			function hidePopup(ele) {
				var popup = ele.closest('.popup');
				popup.hide();
			}
			$(document).on('click', '.popup', function() {
				hidePopup($(this));
			});
