	<div style="display: none">
	    <script>
    

    document.onkeydown = function(evt) {
        evt = evt || window.event;
        var isEscape = false;
        if ('key' in evt) {
            isEscape = evt.key == 'Escape';
        } else {
            isEscape = evt.keyCode == 27;
        }

        if (isEscape) {
            close();
        }
    };

    function close() {
        window.parent.postMessage('ml-accounts-756750-v8v4a4r9i6-webforms-490385-z2d9z1-hide', '*');
    };

    function ml_webform_success_490385 () {
        var $ = ml_jQuery || jQuery;

        $('.row-form').hide();
        $('.row-success').show();
        $(window).scrollTop(0);

        
        if ($('.row-success').find('.success-template').length && $('.success-template').html()) {
            $('.row-success').replaceWith($('.success-template').html());
        }
    }

    
</script>

